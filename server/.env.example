# Application
NODE_ENV=development
PORT=8080
FRONTEND_URL=http://localhost:3000

# MongoDB
MONGO_URI=mongodb://localhost:27017/dental-app

# JWT
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRATION=7d
JWT_COOKIE_EXPIRES_IN=7

# Security
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=900000
AUTH_RATE_LIMIT_MAX=5
AUTH_RATE_LIMIT_WINDOW_MS=900000
BCRYPT_SALT_ROUNDS=12

# CORS
CORS_ORIGIN=http://localhost:3000
CORS_METHODS=GET,HEAD,PUT,PATCH,POST,DELETE
CORS_CREDENTIALS=true

# Email (for notifications)
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=your-email@example.com
EMAIL_PASSWORD=your-email-password
EMAIL_FROM=noreply@dentalclinic.com

# Twilio (for SMS)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number

# Stripe
STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# Insurance Providers
# Delta Dental
DELTA_DENTAL_API_KEY=your-delta-dental-api-key
DELTA_DENTAL_VERIFICATION_URL=https://api.deltadental.com/verification
DELTA_DENTAL_CLAIMS_URL=https://api.deltadental.com/claims

# Cigna
CIGNA_API_KEY=your-cigna-api-key
CIGNA_VERIFICATION_URL=https://api.cigna.com/verification
CIGNA_CLAIMS_URL=https://api.cigna.com/claims

# Aetna
AETNA_API_KEY=your-aetna-api-key
AETNA_VERIFICATION_URL=https://api.aetna.com/verification
AETNA_CLAIMS_URL=https://api.aetna.com/claims

# PDF Generation
PDF_STORAGE_PATH=./storage/pdfs

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs
LOG_MAX_SIZE=20m
LOG_MAX_FILES=14d

# Redis (for caching and session storage)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# Session
SESSION_SECRET=your-session-secret
SESSION_EXPIRES_IN=7d 